#content{
	width: 960px;
	margin:10px auto;
}

.ticket_w{
    width: 880px;
    margin: 0 auto;
    padding: 30px 0;
}
.pl_time{}
.spot_n{ display:inline-block; margin-right:25px;}
.package{ background-color:#fafafa; height:32px; line-height:32px; border-bottom:#CCC 1px solid; margin-bottom:8px;}
.package h3{ font-size:14px; display:inline-block; margin-right:120px;}
.package h3 span{ font-weight:normal; font-size:12px;}
.entticket,.showticket{ display:block; margin:0 0 10px 0;  }
.entticket li,.showticket li{ height:26px; line-height:26px; margin-bottom:15px; padding-left:35px;}
.entticket h4,.showticket h4{ height:26px; line-height:26px; border-bottom:dashed #999 1px; margin-bottom:10px; color:#0068b7;}

#addCart {
