.list_score{ height: 45px; background: url("../images/list/scoreTitle.png") #efefef  left no-repeat;}
.score_cw{ overflow: hidden;}
.score-item{ width: 298px; height: 378px; border: 1px solid #e6e6e6; padding: 10px; float: left; margin-right: 20px; margin-top: 20px;}
.score-item-img{ width: 100%; height: 250px;}
.score-item-img img{ width: 100%; height: 100%;}
.last-item{ margin-right: 0;}
.score-item-title{ font-size: 18px; font-weight: 700; padding: 5px 0; text-align: center;}
.score-item-explian{ text-align: center; padding: 5px 0; width: 240px; margin: 0 auto; border-top: 1px solid #dcdcdc; font-size: 14px; color: #666;}
.score-item-explian em{ color: #ff7e00; font-size: 22px; margin-right: 5px;}
.exchange-btn{ width: 200px; height: 40px; line-height: 40px; text-align: center; display: block; margin: 0 auto; background: url("../images/list/exchange-btn.png") no-repeat; color: #fff; font-size: 18px;}
.exchange-btn:hover{ text-decoration: none;}

.date-panel{width: 140px; height: 24px; line-height: 24px; padding: 0 10px; background-color: #fff; border: 1px solid #ccc; display: inline-block; vertical-align: middle; background: url("../images/order/calendar-icon.png") 140px center no-repeat; color: #333;}
.date-panel:hover{ color: #333; text-decoration: none;}
.date-panel em{ margin: 0 8px;}

.address-panel{ display: inline-block;}

.tips-layer{ width: 500px; height: 300px; position: fixed; top:50%; left: 50%; margin: -250px 0 0 -150px; background-color: #fff; border-radius: 10px; z-index: 20; display: none;}
.tips-layer-header{ text-align: right; padding: 10px;}
.tips-content{ font-size: 14px; text-align: center; height: 80px;}
.tips-handle{ text-align: center;}
.tips-handle a{ display: inline-block; width: 140px; height: 32px; text-align: center; line-height:32px; border-radius: 15px; color: #fff; font-size: 16px; margin: 0 10px;}
.ent-btn{ background-color: #ffc000;}
.cancel-btn{ background-color: #ccc;}
.icon-iconfont-32pxchaxian{ display: inline-block; width: 32px; height: 32px; background: url("../images/order/close.png") no-repeat;}