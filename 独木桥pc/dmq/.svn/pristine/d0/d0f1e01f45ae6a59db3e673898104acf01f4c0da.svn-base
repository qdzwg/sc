
.czone {
	border: 1px solid #eee;
	position: relative;
	padding: 10px 0 10px;
}

.step,.step_t{
	width: 960px;
	height: 51px;
	position: relative;
	left: -11px;
}
.step{background: url(../images/common/ydbar1.gif) no-repeat 0 0;}
.step_t{background: url(../images/common/ydbar2.gif) no-repeat 0 0;}
.step div,.step_t div{
	float: left;
	line-height: 40px;
	color: #fff;
	font-size: 14px;
	text-align: center;
}
.s1{width: 130px;}
.s2{width: 140px;}
.s3{width: 130px;}
.s4{width: 135px;}
.s5{width: 160px;}
.s6{width: 145px;}

.t1{width: 135px;}
.t2{width: 135px;}
.t3{width: 130px;}
.t4{width: 135px;}
.t5{width: 140px;}
.t6{width: 130px;}
.t7{width: 115px;}

.step .son,.step_t .son{font-weight: bold;font-size: 16px;}

.cm_title{
	line-height: 30px;
}

.cm_wrapper{
	width: 938px;
	margin: 0 auto;
}
.cm_content{
	width: 932px;
	border: 3px solid #BAD7E7;
}

.cm_sp{	background: url(../images/common/re.gif) repeat-y 190px 0;
}
.cm_cl{
	float: left;
	width: 240px;
}
.cm_cr{
    width: 670px;
    float: right;
    padding: 10px 0;
    margin-right: 10px;
    _display:inline;
}

.cm_cli{
	width: 220px;
	margin: 0 auto;
	padding: 10px 0;
}


.cm_name{
	border-bottom: 2px solid #DDDDDD;
}
.cm_name h2{
	font-family: Microsoft Yahei;
	font-size: 18px;
	border-bottom: 2px solid #0E5788;
	margin-bottom: -2px;
	float: left;
	font-weight: normal;
}


.cm_spot{
	padding-top: 20px;
	margin-bottom: 20px;
}


ul.normal_list li {
    padding-bottom:10px;
    font-size:13px;
}

ul.normal_list li span {
    display: inline-block;
}
.g_span{color: #666666;}

.wl {
    padding-right: 5px;
    text-align: right;
    width: 105px;
}
.w2 {
    padding-right: 5px;
    text-align: right;
    width: 135px;
}

.text{
	border: 1px solid #ADACB1;
	padding: 2px;
	height:22px;
