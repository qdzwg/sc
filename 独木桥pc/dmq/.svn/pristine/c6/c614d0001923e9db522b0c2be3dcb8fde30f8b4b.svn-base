block scriptspage
    script(src=baseUrl+'/javascripts/login.js')
    script(type='text/javascript').
            var win = $(window);
            var doc = $(document);
            $(function(){
            check_load();
            win.scroll(function(){
            check_load();
            }).resize(function(){
            check_load();
            });
            });
            function check_load(){
            $('.lz').each(function(){
            if ((win.height()+doc.scrollTop())>$(this).offset().top){
            var img = new Image();
            var tval = $(this).attr('lzimg');
            var tobj = $(this);
            img.onload = function(){
            tobj.hide();
            tobj.find('.hold').show();
            tobj.width('auto').css({'background':'url('+tval+') no-repeat center top'});
            tobj.fadeIn('fast');
            tobj.removeClass('lz');
            }
            img.src = tval;
            }
            });
            }