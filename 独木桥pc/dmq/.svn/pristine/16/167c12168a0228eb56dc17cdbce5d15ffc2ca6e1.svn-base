#main{
	width: 1000px;
	margin: 10px auto;
}

#breadcrumb{
	width: 100%;
	margin-bottom: 10px;
}
#breadcrumb a{
	color: #0053AA;
}

#intro{
	width: 1000px;
	height: 395px;
	margin-bottom: 10px;
	position: relative;
	overflow: hidden;
	background: #eee url("../images/details/loader.gif") no-repeat center center;
}
#scroll_w img{
	width: 1000px;
	height: 395px;
	vertical-align: top;
}

#scroll_w span{background-color: #333;color:#fff;cursor:pointer;padding:0 6px;height:20px;line-height:20px;font-size:12px;text-align:center;position: absolute;font-family: Arial;}
#scroll_w span.koosel{background-color: #ff6600;}

.td_name{color:#0066CC;}
.or{color: #ff6600;}

.bc_l{
	float: left;
}

#scroll_wrapper{
	background: #ccc;
	width: 1000px;
	height: 395px;
	position: relative;
	margin-bottom: 10px;
	overflow: hidden;
}
#scroll_b{
	width: 1000px;
	height: 395px;
	position: relative;
	background: url("../images/details/loader.gif") no-repeat center center;
}
#scroll_b a{
	display: block;
	width: 1000px;
	height: 395px;
	position: absolute;
	left: 0;
	top: 0;
	display: none;
}
#scroll_s{
	width: 988px;
	height: 60px;
	position: absolute;
	top: 335px;
	right:0;
	display: none;
	padding-left: 12px;
}
#scroll_s img{
	float: left;
	cursor: pointer;
	border: 2px solid #757575;
	margin-right: 8px;
	margin-top: 4px;
}
#scroll_s img.sc_sel{
	border-color: #FFEC00;
}

#c_wrapper{
	width: 1000px;
	position: relative;
	margin: 0 auto;
}

.list_h {
    background: url("../images/details/snav_re.png") repeat-x scroll 0 0 #F3652F;
    height: 35px;
}
.h_txt {
    background: #FFFFFF;
    color: #333333;
    float: left;
    font-size: 14px;
    font-weight: bold;
    height: 32px;
    line-height: 32px;
    margin:3px 0 0 10px;
    padding: 0 15px;
}

.group_w {
