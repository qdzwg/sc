
#content{
	width: 960px;
	margin:10px auto;
}


.pictxt {
    width: 220px;
    padding-bottom: 10px;
    border-bottom: 1px dotted #DEDEDE;
}
.pictxt dt {
    float: left;
    overflow: hidden;
    width: 80px;
}
.pictxt dt a {
    color: #1B66C3;
    font: bold 14px simsun;
    text-decoration: none;
}
.pictxt dt a img {
    border: 0 none;
    width: 80px;
}
.pictxt dd {
    color: #888888;
    margin-left: 90px;
    text-align: left;
}
.pictxt .hn_d2 {
    color: #1B66C3;
    font: bold 12px/16px Arial;
}
.pictxt .hn_d3 {
    color: #888888;
}

.dlist{
    width: 220px;
    margin: 0 auto;
    line-height: 24px;
}




.price_list {
    padding-left: 110px;
}

.price_list .arr, .price_list .arr th, .price_list .arr td {
    border: 1px solid #FBECAC;
    border-collapse: collapse;
}
.price_list .arr table, .price_list .arr table th, .price_list .arr table td{
	border:none;
}

.price_list .arr th {
    background: none repeat scroll 0 0 #FDF7DD;
    height: 24px;
    text-align: center;
    width: 100px;
    font-weight: normal;
}
.price_list td {
    line-height: 1.4em;
    text-align: center;
}
.price_list td table td{
	padding: 5px 0;
}

.name_list_w{
    float: left; 
}
.name_span{
    float:left;
}
.name_block{
    float: left;
    margin-right: 20px;
    padding-bottom: 10px;
}
.name_list_w{
    width: 450px;
}
.name_sel{
    width: 100px;
    padding-right: 16px;
    background: url(../images/common/icons.png) no-repeat 105px center;
}



/*修改日期弹出层*/
.date_change{ margin-left:40px; background-color:#FFF; width:530px; border:#ccc 1px solid; border-top-left-radius:3px;border-top-right-radius:3px;}
.date_change_tit{ margin-top:1px; background-color:#fbfbfb; height:32px; line-height:32px; color:#666; font-weight:bold; padding-left:20px; border-bottom:#CCC 1px solid;}
.date_change_con{ background-color:#FFF; padding:20px 25px; color:#333;}
.date_change_con input.btn{ background:url(../images/common/confirm.png) no-repeat; color:#FFF; font-weight:bold; width:60px; height:25px; line-height:25px; font-size:12px; text-align:center; border:none; }
.date_change_con input.sr{ width:120px;}
.cha{ display:inline-block; text-indent:999px; float:right; margin : 6px 12px 0 0; overflow:hidden; background:url(../images/common/cha_png.png) center center no-repeat; width:20px; height:20px;}


.order_b{
    border:none;
    width: 122px;
    height:36px;
    background:url(../images/order/subbtn.gif) no-repeat 0 0; vertical-align: middle; display: inline-block;
}