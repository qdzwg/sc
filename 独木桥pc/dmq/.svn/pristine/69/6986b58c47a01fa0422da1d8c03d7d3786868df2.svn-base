/***** pagination begin *****/

.pagination
{
    clear:both;
    height:40px;
    font-size:12px;
    color:#404040;
    width:944px;
    padding:35px 0 15px;
    font-family: Microsoft Yahei;
}
.page-bottom{float:right}
.pagination a,.page-prev,.page-cur,.page-start,.page-end,.page-break,.page-skip{float:left;color:#404040;margin:0 3px}
.pagination a,.page-prev,.page-start,.page-cur,.page-end,.page-skip{height:22px;padding:6px 10px 0;line-height:16px}
.page-start{width:auto}
.page-cur,.pagination a,.page-end{border:1px solid #C4C4C4;}
.page-cur{color:#fff;}
.pagination a:hover{color:#404040;background-color:#8A704D;color: #fff;}
.page-start,.pagination .page-prev{border:1px solid #C4C4C4}
.page-cur{background-color:#8A704D}
.page-break{padding:0 4px}

/***** pagination end *****/

